select concat_ws('.',substr(`presupuesto`.`codigoadm`,1,2),substr(`presupuesto`.`codigoadm`,4,2),substr(`presupuesto`.`codigoadm`,10,2),if((`presupuesto`.`codigopres` like '%.00'),replace(`presupuesto`.`codigopres`,'.00',''),`presupuesto`.`codigopres`)) AS `codigo`,`presupuesto`.`denominacion` AS `denominacion`,`presupuesto`.`tipo` AS `fondo`,`presupuesto`.`asignacion` AS `asignacion`,`presupuesto`.`aumento` AS `aumento`,`presupuesto`.`disminucion` AS `disminucion`,`presupuesto`.`traslados` AS `traslados`,`presupuesto`.`comprometido` AS `comprometido`,`presupuesto`.`opago` AS `causado`,`presupuesto`.`pagado` AS `pagado`,concat_ws('.',if((`presupuesto`.`codigopres` like '%.00'),replace(`presupuesto`.`codigopres`,'.00',''),`presupuesto`.`codigopres`)) AS `codigopres`,`presupuesto`.`orden` AS `orden`,`presupuesto`.`inversion` AS `inversion` from `presupuesto`
