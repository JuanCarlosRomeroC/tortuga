select concat(substr(`estruadm`.`codigo`,1,2),convert(convert(if((length(`estruadm`.`codigo`) > 2),'.','') using latin1) using utf8),substr(`estruadm`.`codigo`,4,2),convert(convert(if((length(`estruadm`.`codigo`) > 8),'.','') using latin1) using utf8),substr(`estruadm`.`codigo`,10,2)) AS `codigo`,`estruadm`.`denominacion` AS `denominacion` from `estruadm` group by concat(substr(`estruadm`.`codigo`,1,2),convert(convert(if((length(`estruadm`.`codigo`) > 2),'.','') using latin1) using utf8),substr(`estruadm`.`codigo`,4,2),convert(convert(if((length(`estruadm`.`codigo`) > 8),'.','') using latin1) using utf8),substr(`estruadm`.`codigo`,10,2))
