select `a`.`fecha` AS `fecha`,`a`.`des` AS `des`,`a`.`observa` AS `observa`,`a`.`cod_prov` AS `cod_prov`,`a`.`numero` AS `numero`,`a`.`status` AS `status`,`a`.`modo` AS `modo`,`a`.`codigoadm` AS `codigoadm`,`a`.`tipo` AS `tipo`,`a`.`codigopres` AS `codigopres`,`a`.`ordinal` AS `ordinal`,`a`.`faudis` AS `faudis`,`a`.`ftrasla` AS `ftrasla`,`a`.`fcomprome` AS `fcomprome`,`a`.`fcausado` AS `fcausado`,`a`.`fopago` AS `fopago`,`a`.`fpagado` AS `fpagado`,`a`.`frendi` AS `frendi`,`a`.`comprometido` AS `comprometido`,`a`.`causado` AS `causado`,`a`.`opago` AS `opago`,`a`.`pagado` AS `pagado`,`a`.`aumento` AS `aumento`,`a`.`disminucion` AS `disminucion`,`a`.`traslados` AS `traslados`,`a`.`asignacion` AS `asignacion`,`a`.`ccomprometido` AS `ccomprometido`,`a`.`ccausado` AS `ccausado`,`a`.`copago` AS `copago`,`a`.`cpagado` AS `cpagado`,`b`.`denominacion` AS `denoadm` from (`view_pres_s2` `a` join `estruadm` `b` on((`a`.`codigoadm` = `b`.`codigo`)))
