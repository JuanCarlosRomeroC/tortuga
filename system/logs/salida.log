SELECT carg.descrip ncargo,b.ingreso,d.descrip descripd,b.divi,d.codigoadm,d.fondo,a.codigo, a.concepto, a.monto, CONCAT_WS(' ',b.nombre,b.apellido) nombre, c.descrip, CONCAT(b.nacional, b.cedula) cedula,a.valor*(a.tipo='A') as avalor,a.valor*(a.tipo='D') as dvalor, a.valor*(a.tipo IN ('A','D')) as neto,a.valor*(a.tipo='O') as ovalor FROM prenom  a  JOIN  pers b ON a.codigo=b.codigo JOIN divi d ON b.divi=d.division  JOIN conc c ON a.concepto=c.concepto LEFT JOIN carg ON b.cargo=carg.cargo WHERE a.valor<>0 ORDER BY b.divi,codigo, concepto 