select `v_comproxcausar_s1`.`ocompra` AS `ocompra`,`v_comproxcausar_s1`.`compromiso` AS `compromiso`,`v_comproxcausar_s1`.`opago` AS `opago`,`v_comproxcausar_s1`.`esiva` AS `esiva`,`v_comproxcausar_s1`.`codigoadm` AS `codigoadm`,`v_comproxcausar_s1`.`codigopres` AS `codigopres`,`v_comproxcausar_s1`.`fondo` AS `fondo`,sum(`v_comproxcausar_s1`.`compras`) AS `compras`,sum(`v_comproxcausar_s1`.`pagos`) AS `pagos`,(sum(`v_comproxcausar_s1`.`compras`) - sum(`v_comproxcausar_s1`.`pagos`)) AS `xcausar` from `v_comproxcausar_s1` group by `v_comproxcausar_s1`.`ocompra`,`v_comproxcausar_s1`.`codigoadm`,`v_comproxcausar_s1`.`codigopres`,`v_comproxcausar_s1`.`fondo`,(`v_comproxcausar_s1`.`esiva` = 'N') having (`compras` > `pagos`)
