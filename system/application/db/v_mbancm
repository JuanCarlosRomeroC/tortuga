select `a`.`multiple` AS `multiple`,`a`.`codbanc` AS `codbanc`,`a`.`tipo_doc` AS `tipo_doc`,group_concat(`a`.`cheque` separator ',') AS `cheque`,date_format(`a`.`fecha`,'%d/%m/%Y') AS `fecha`,sum(`a`.`monto`) AS `monto`,`a`.`observa` AS `observa`,`b`.`numcuent` AS `numcuent`,`b`.`banco` AS `banco`,`a`.`benefi` AS `benefi` from (`mbanc` `a` join `banc` `b` on((`a`.`codbanc` = `b`.`codbanc`))) where ((`a`.`multiple` > 0) and (`a`.`status` in ('J2','A2'))) group by `a`.`multiple`,`a`.`tipo_doc`,`a`.`codbanc`,`a`.`fecha` order by `a`.`multiple` desc
