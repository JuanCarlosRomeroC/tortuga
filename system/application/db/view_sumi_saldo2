select `b`.`caub` AS `caub`,`a`.`codigo` AS `codigo`,`a`.`cantidad` AS `cantidad` from (`itsuminr` `a` join `suminr` `b` on((`a`.`numero` = `b`.`numero`))) where (`b`.`status` = 'C') union all select `b`.`caub` AS `caub`,`a`.`codigo` AS `codigo`,(-(1) * `a`.`cantidad`) AS `-1*a.cantidad` from (`itsumine` `a` join `sumine` `b` on((`a`.`numero` = `b`.`numero`))) where (`b`.`status` = 'C')
